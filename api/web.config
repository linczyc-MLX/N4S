<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <!--
      CORS headers are handled by PHP in config.php
      DO NOT add CORS headers here - it causes duplicate '*, *' error
    -->

    <!-- Remove WebDAV which can interfere with PUT/DELETE -->
    <modules runAllManagedModulesForAllRequests="true">
      <remove name="WebDAVModule" />
    </modules>
    <handlers>
      <remove name="WebDAV" />
    </handlers>
  </system.webServer>
</configuration>
